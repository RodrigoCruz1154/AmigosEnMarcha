<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bonobostudios.amigosenmarcha">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DinamicsActivity"></activity>
        <activity android:name=".ToolsActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ListasActivity" />
        <activity android:name=".JornadaActivity" />
        <activity android:name=".CoordLauncherActivity" />
        <activity android:name=".NormalLauncherActivity" />
        <activity
            android:name=".PopUp"
            android:theme="@style/AppTheme.CustomThemePopUp" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>